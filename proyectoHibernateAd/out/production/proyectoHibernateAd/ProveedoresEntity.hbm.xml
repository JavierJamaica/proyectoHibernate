<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entidades.ProveedoresEntity" table="proveedores" schema="proyectohibernate">
        <id name="idProveedores">
            <column name="idProveedores" sql-type="int"/>
        </id>
        <property name="nombreProv">
            <column name="nombreProv" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="apellidosProv">
            <column name="apellidosProv" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="direccionPostal">
            <column name="direccionPostal" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>